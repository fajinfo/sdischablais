  <div id="calendar" class="fc fc-ltr fc-unthemed">
    <div class="fc-view-container">
        <div class="fc-view fc-agendaWeek-view fc-agenda-view">
            <table>
                <thead>
                    <tr>
                        <td class="fc-widget-header">
                            <div class="fc-row fc-widget-header" style="border-right-width: 1px; margin-right: 16px;">
                                <table>
                                    <thead>
                                        <tr>
                                            <th class="fc-axis fc-widget-header" style="width: 43px;"></th>
                                            {% for classe, jour in headers %} 
                                            <th class="fc-day-header fc-widget-header fc-{{ classe }}">{{ jour }}</th>
                                            {% endfor %}
                                            </tr>
                                        </thead>
                                    </table>
                                </div>
                            </td>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class="fc-widget-content">
                                <div class="fc-time-grid-container fc-scroller" style="height: 859px;">
                                    <div class="fc-time-grid">
                                        <div class="fc-bg">
                                            <table>
                                                <tbody>
                                                    <tr>
                                                        <td class="fc-axis fc-widget-content" style="width: 43px;"></td>
                                                        {% for date in dates %}
                                                        <td class="fc-day fc-widget-content fc-sun fc-past" data-date="{{ date }}"></td>
                                                        {% endfor %}
                                                        </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                        <div class="fc-slats">
                                            <table>
                                                <tbody>
                                                {% for heure in heures %}
                                                    <tr>
                                                        <td class="fc-axis fc-time fc-widget-content" style="width: 43px;">
                                                            <span>{{ heure }}</span>
                                                        </td>
                                                        <td class="fc-widget-content"></td>
                                                    </tr>
                                                    <tr class="fc-minor">
                                                        <td class="fc-axis fc-time fc-widget-content" style="width: 43px;"></td>
                                                        <td class="fc-widget-content"></td>
                                                    </tr>
                                                {% endfor %}
                                                </tbody>
                                            </table>
                                        </div>
                                        <div class="fc-content-skeleton">
                                        <table>
                                            <tbody>
                                                <tr>
                                                    <td class="fc-axis" style="width:43px;"></td>
                                                    {% for day in events %}
                                                    <td>
                                                        <div class="fc-event-container">
                                                            {% for zindex, event in day %}
                                                            <a class="label-{{ event.vhc }} fc-time-grid-event fc-event fc-start fc-end" style="top: {{ event.top }}px; bottom: -{{ event.bottom }}px; left: {{ event.left }}%; right: {{ event.right }}%; z-index: {{ zindex }};">
                                                                <div class="fc-content">
                                                                    <div class="fc-title"><img src="/img/missions/{{ event.mission }}" /> {{ event.title }}</div>
                                                                    <div class="fc-time" data-full="{{ event.startAt }}" data-start="{{ event.startAt }}">
                                                                        <span>{{ event.startAt }}</span>
                                                                    </div>
                                                                </div>
                                                                <div class="fc-bg"></div>
                                                            </a>
                                                            {% endfor %}
                                                        </div>
                                                    </td>
                                                    {% endfor %}
                                                </tr>
                                            </tbody>
                                        </table>                                           
                                        </div>
                                    </div>
                                </div>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>