{% extends "JspAdminBundle::layout.html.twig" %}

{% block corps %}
<h1>LIENS</h1>
<div class="panel panel-danger">
  <div class="panel-heading">
    <h3 class="panel-title">Voulez vous vraiment supprimer le lien ?</h3>
  </div>
  <div class="panel-body">
    <p>{{ lien.nom }} </p>
	<div class="btn-group">
		<a href="{{ path('jsp_admin_liens_liste') }}" class="btn btn-success">Retour</a>
		<a href="{{ path('jsp_admin_liens_supprimerok', { 'id': lien.id }) }}" class="btn btn-danger">Supprimer</a>
	</div>
  </div>
</div>
{% endblock %}
